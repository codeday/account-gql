{
    "compilerOptions": {
        "target": "ES2019",
        "lib": [
            "ES2019"
        ],
        "module": "commonjs",
        "strict": true,
        "rootDir": "src",
        "outDir": "dist",
        "plugins": [],
        "typeRoots": [
            "node_modules/@types",
            "types",
            "src/_types"
        ],
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "strictPropertyInitialization": false
    },
    "include": [
        "src",
    ]
}
